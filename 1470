class Solution(object):
    def shuffle(self, nums, n):
        """
        :type nums: List[int]
        :type n: int
        :rtype: List[int]
        """
        result_arr=[]
        
        for i in range(0,len(nums)/2):
            result_arr.append(nums[i])
            result_arr.append(nums[i+len(nums)/2])
            
        return result_arr

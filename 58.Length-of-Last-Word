class Solution(object):
    def lengthOfLastWord(self,s):
        """
        :type s: str
        :rtype: int
        """
        
        index=-1
        v=s.split(" ")
        leng=len(v[index])
        
        while(leng == 0):
            index-= -1
            leng=len(v[index])
            
            
        
        return leng 
    
    
    
